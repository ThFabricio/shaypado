<div id="<%= dom_id aluno %>">
  <p>
    <strong>Nome:</strong>
    <%= aluno.nome %>
  </p>

  <p>
    <strong>Logradouro:</strong>
    <%= aluno.endereco.logradouro %>
  </p>

  <p>
    <strong>Cep:</strong>
    <%= aluno.endereco.cep %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= aluno.cpf %>
  </p>

  <p>
    <strong>Data nascimento:</strong>
    <%= aluno.data_nascimento %>
  </p>

  <p>
    <strong>Contato:</strong>
    <%= aluno.contato %>
  </p>

</div>
